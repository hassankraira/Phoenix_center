{
  "name": "phoenix-center",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "ng serve --host 0.0.0.0",
    "start:backend": "cd phoenix_backend && npm start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "arabic-reshaper": "^1.1.0",
    "bidi-js": "^1.0.3",
    "dayjs": "^1.11.19",
    "dayjs-plugin-utc": "^0.1.2",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "rxjs": "~7.8.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^25.2.0",
    "angular-cli-ghpages": "^3.0.2",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
